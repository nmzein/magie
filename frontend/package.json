{
	"name": "magie",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 4000",
		"prod": "vite build && vite preview --port 4000",
		"ci:dev": "vite dev --port 4000",
		"ci:build": "vite build",
		"ci:prev": "vite preview --port 4000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "3.2.2",
		"@sveltejs/kit": "2.5.18",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/eslint": "9.6.0",
		"@types/three": "0.167.1",
		"eslint": "9.8.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.43.0",
		"globals": "15.8.0",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.6",
		"sass": "1.77.8",
		"svelte": "5.0.0-next.200",
		"svelte-check": "3.8.4",
		"tslib": "2.6.3",
		"typescript": "5.5.4",
		"typescript-eslint": "^8.0.0-alpha.45",
		"vite": "5.3.5",
		"workbox-precaching": "7.1.0"
	},
	"dependencies": {
		"@iconify/json": "2.2.231",
		"canvas": "2.11.2",
		"three": "0.167.0",
		"unplugin-icons": "0.19.0",
		"ws": "8.18.0"
	},
	"type": "module"
}
