{
	"name": "magie",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 4000",
		"prod": "vite build && vite preview --port 4000",
		"ci:dev": "vite dev --port 4000",
		"ci:build": "vite build",
		"ci:prev": "vite preview --port 4000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "3.2.1",
		"@sveltejs/kit": "2.5.10",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/three": "0.165.0",
		"@types/eslint": "8.56.10",
		"eslint": "9.4.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.39.3",
		"globals": "15.4.0",
		"prettier": "3.3.2",
		"prettier-plugin-svelte": "3.2.4",
		"sass": "1.77.5",
		"svelte": "5.0.0-next.154",
		"svelte-check": "3.8.0",
		"tslib": "2.6.3",
		"typescript": "5.4.5",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "5.2.13",
		"workbox-precaching": "7.1.0"
	},
	"dependencies": {
		"@iconify-json/fluent": "1.1.56",
		"@iconify-json/material-symbols": "1.1.82",
		"@iconify-json/material-symbols-light": "1.1.22",
		"@iconify/json": "2.2.218",
		"canvas": "2.11.2",
		"three": "0.165.0",
		"unplugin-icons": "0.19.0",
		"ws": "8.17.0"
	},
	"type": "module"
}
